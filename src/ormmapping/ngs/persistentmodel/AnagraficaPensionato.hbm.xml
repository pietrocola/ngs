<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ngs.persistentmodel.AnagraficaPensionato" table="AnagraficaPensionato" lazy="false">
		<id name="codiceFiscale" column="ID" type="string">
			<generator class="assigned">
			</generator>
		</id>
		<property name="nome" column="Nome" type="string" length="255" not-null="false" lazy="false"/>
		<property name="cognome" column="Cognome" type="string" length="255" not-null="false" lazy="false"/>
		<property name="pensione" column="Pensione" type="boolean" length="1" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
