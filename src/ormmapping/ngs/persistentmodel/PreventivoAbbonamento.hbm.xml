<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ngs.persistentmodel.PreventivoAbbonamento" table="PreventivoAbbonamento" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="politicaSconto" cascade="lock" column="PoliticaScontoAbbonamentonomePolitica" class="ngs.persistentmodel.PoliticaScontoAbbonamento" not-null="false" lazy="no-proxy">
		</many-to-one>
		<many-to-one name="descAbb" cascade="lock" column="DescrizioneAbbonamentonomeAbbonamento" class="ngs.persistentmodel.DescrizioneAbbonamento" not-null="false" lazy="no-proxy">
		</many-to-one>
		<property name="prezzo" column="Prezzo" type="float" not-null="true" lazy="false"/>
		<property name="numeroMesi" column="NumeroMesi" type="integer" length="11" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
